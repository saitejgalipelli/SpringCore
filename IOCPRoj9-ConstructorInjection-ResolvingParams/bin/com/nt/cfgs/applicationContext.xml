  <bean id="mk"  class="com.nt.beans.Marks">
      <constructor-arg index="2" value="90"/>
      <constructor-arg index="0" value="80"/>
      <constructor-arg index="1"  value="70"/>
  </bean>
  
  <bean id="per" class="com.nt.beans.Person">
      <constructor-arg type="long"  value="989898987654353"/>
      <constructor-arg type="int"  value="1001"/>
      <constructor-arg type="java.lang.String"  value="ramesh"/>
